<%= semantic_form_for @schedule do |f| %>
<fieldset>
    <legend>Crear Jornada</legend>

    <%= f.semantic_errors %>
    <%= f.inputs do %>
        <%= f.input :season %>
        <%= f.input :number %>
        <%= f.input :start_at, :as => :string, :input_html => {:class => "span2",  "data-behaviour" => "datepicker"}  %>    
        <%= f.input :end_at, :as => :string, :input_html => {:class => "span2",  "data-behaviour" => "datepicker"}  %>        
    <% end %>
    <%= f.actions do %>
        <%= f.action :submit,:button_html => { :class => "btn btn-large btn-primary"} %>        
    <% end %>
</fieldset>
<% end %>
